 generator client{
    provider = "prisma-client-js"
 }

 datasource db{
    provider = "mysql"
    url = env("DATABASE_URL")
  }
    
model User{
    id Int @id @default(autoincrement())
    firstname String
    lastname String
    age Int 
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
  }
// └─$ npx prisma generate
// └─$ npx prisma migrate dev --name init --create-only

  
   